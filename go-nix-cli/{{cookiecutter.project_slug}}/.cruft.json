{
  "template": "{{ cookiecutter._template | default('unknown') }}",
  "commit": "{{ cookiecutter._commit | default('unknown') }}",
  "context": {
    "cookiecutter": {
      "project_name": "{{ cookiecutter.project_name }}",
      "project_slug": "{{ cookiecutter.project_slug }}",
      "project_description": "{{ cookiecutter.project_description }}",
      "github_owner": "{{ cookiecutter.github_owner }}",
      "github_repo": "{{ cookiecutter.github_repo }}",
      "module_path": "{{ cookiecutter.module_path }}",
      "author_name": "{{ cookiecutter.author_name }}",
      "author_email": "{{ cookiecutter.author_email }}",
      "license": "{{ cookiecutter.license }}",
      "use_cgo": "{{ cookiecutter.use_cgo }}",
      "use_homebrew": "{{ cookiecutter.use_homebrew }}",
      "use_sqlc": "{{ cookiecutter.use_sqlc }}",
      "go_version": "{{ cookiecutter.go_version }}"
    }
  },
  "directory": "go-nix-cli",
  "skip": [
    ".github/.release-please-manifest.json",
    "CHANGELOG.md",
    "README.md",
    ".goreleaser.yaml",
    "cmd",
    "internal",
    "db",
    "test",
    "sqlc.yaml",
    "main.go",
    "go.mod",
    "go.sum",
    "gomod2nix.toml",
    "flake.lock"
  ]
}
